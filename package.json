  "description": "A Redux based GUI for jsPsych",
  "main": "index.js",
  "jest": {
    "verbose": true,
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json",
      "es6"
    ]
  },
  "scripts": {
