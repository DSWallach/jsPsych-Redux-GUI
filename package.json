  },
  "homepage": "https://github.com/jsPsych/jsPsych-Redux-GUI#readme",
  "dependencies": {
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "material-ui": "^0.17.1",
    "mousetrap": "^1.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-form": "^1.2.6",
    "react-redux": "^4.4.6",
    "react-tap-event-plugin": "^2.0.1",
